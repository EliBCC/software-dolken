		<module>:platform:uart:1</module>
		<module>:platform:uart:2</module>
		<module>:platform:spi:1</module>
		<module>:platform:spi:2</module>
		<module>:platform:i2c:1</module>
		<module>:platform:i2c:2</module>
		<module>:driver:nrf24</module>
